{
  "name": "chat-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:scss && npm run lint:html",
    "lint:ts": "eslint --color -c .eslintrc --ext .ts .",
    "lint:scss": "npx stylelint \"src/**/*.scss\" ",
    "lint:html": "htmlhint --config .htmlhintrc",
    "format": "prettier \"./src/**/*.{ts,js,html,scss}\" --write",
    "dev:ssr": "ng run chat-frontend:serve-ssr",
    "serve:ssr": "node dist/chat-frontend/server/main.js",
    "build:ssr": "ng build --configuration production && ng run chat-frontend:server",
    "prerender": "ng run chat-frontend:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.6",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/platform-server": "^15.2.6",
    "@angular/router": "^15.2.0",
    "@angular/service-worker": "^15.2.0",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@ngrx/effects": "^15.4.0",
    "@ngrx/entity": "^15.4.0",
    "@ngrx/router-store": "^15.4.0",
    "@ngrx/store": "^15.4.0",
    "@ngrx/store-devtools": "^15.4.0",
    "@nguniversal/express-engine": "^15.2.1",
    "@stomp/rx-stomp": "^2.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.15.2",
    "jwt-decode": "^3.1.2",
    "ng2-file-upload": "^4.0.0",
    "ngx-image-compress": "^15.1.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "ws": "^8.13.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "^15.2.0",
    "@ngrx/schematics": "^15.2.0",
    "@nguniversal/builders": "^15.2.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "htmlhint": "^1.1.4",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "2.8.8",
    "stylelint": "^15.6.1",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "~4.9.4"
  }
}
